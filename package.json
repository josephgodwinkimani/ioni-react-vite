{
  "name": "ionic-react-vite",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "author": "Joseph Godwin Kimani <josephgodwinke@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/josephgodwinkimani/ionic-react-vite-starter.git"
  },
  "keywords": [
    "vite",
    "react",
    "ionic",
    "eslint",
    "prettier",
    "hybrid",
    "WebdriverIO",
    "WebDriver",
    "appium",
    "hybrid"
  ],
  "bugs": {
    "url": "https://github.com/josephgodwinkimani/ionic-react-vite-starter/issues"
  },
  "homepage": "https://github.com/josephgodwinkimani/ionic-react-vite-starter#README.MD",
  "scripts": {
		"start": "npm install && husky install",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "clean": "rimraf build && rimraf node_modules",
    "sync": "vite build && ionic capacitor sync android --no-build",
    "lint": "eslint src/**/*.{js,jsx}",
    "fix": "eslint src/**/*.{js,jsx} --fix",
    "format": "prettier src/**/*.{js,jsx} --write",
    "browser.test": "wdio run ./wdio.browser.conf.js",
    "android.app": "wdio config/wdio.android.app.conf.ts",
    "android.browser": "wdio config/wdio.android.browser.conf.ts",
    "android.browserstack.app": "wdio ./config/browserstack/wdio.android.bs.app.conf.ts",
    "ios.app": "wdio config/wdio.ios.app.conf.ts",
    "ios.sauce.simulators.app.eu": "REGION=eu wdio ./config/saucelabs/wdio.ios.simulators.app.conf.ts",
    "ios.sauce.simulators.app.us": "wdio ./config/saucelabs/wdio.ios.simulators.app.conf.ts",
    "ios.browser": "wdio config/wdio.ios.browser.conf.ts"
  },
  "dependencies": {
    "@capacitor/android": "4.6.3",
    "@capacitor/app": "4.1.0",
    "@capacitor/cli": "4.6.3",
    "@capacitor/core": "4.6.3",
    "@capacitor/haptics": "4.1.0",
    "@capacitor/ios": "4.6.3",
    "@capacitor/keyboard": "4.1.0",
    "@capacitor/status-bar": "4.1.0",
    "@ionic/react": "^6.0.0",
    "@ionic/react-router": "^6.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@capacitor/cli": "4.6.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "@wdio/appium-service": "^7.19.5",
    "@wdio/browser-runner": "^8.5.2",
    "@wdio/browserstack-service": "^7.19.7",
    "@wdio/cli": "^7.19.7",
    "@wdio/globals": "^7.20.8-alpha.504",
    "@wdio/local-runner": "^7.19.7",
    "@wdio/mocha-framework": "^7.19.7",
    "@wdio/sauce-service": "^7.19.7",
    "@wdio/spec-reporter": "^7.19.7",
    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-wdio": "^7.19.4",
    "husky": "^8.0.3",
    "prettier": "^2.7.1",
    "rimraf": "^4.1.2",
    "ts-node": "^10.8.0",
    "typescript": "^4.6.4",
    "vite": "^4.1.0",
    "vite-plugin-pwa": "^0.14.4"
  }
}